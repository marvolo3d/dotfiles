[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=OneDark
Font=Inconsolata-g for Powerline,10,-1,5,57,0,0,0,0,0

[Cursor Options]
CursorShape=2

[General]
Command=/bin/zsh
Directory=$HOME
Environment=TERM=xterm-256color,COLORTERM=truecolor
Icon=konsole
Name=marvolo
Parent=FALLBACK/
StartInCurrentSessionDir=false
TerminalColumns=120
TerminalRows=40

[Interaction Options]
TrimTrailingSpacesInSelectedText=true

[Scrolling]
HistoryMode=1
ScrollFullPage=false

[Terminal Features]
BidiRenderingEnabled=true
BlinkingCursorEnabled=true
